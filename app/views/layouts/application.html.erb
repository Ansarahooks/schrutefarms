<!DOCTYPE html>
<html>
  <head>
    <title>SchruteFarmsBnb</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <link href="https://fonts.googleapis.com/css?family=Work+Sans:400"/>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <header>
      <div class="container">
        <a href='/'><img src="https://images-na.ssl-images-amazon.com/images/I/61wDOQ8%2BT6L._SL1273_.jpg" alt="logo" class="logo" style="width:68px;height:72px;"></a>
          <nav>
            <ul>
              <li><a href="/">Home</a></li>
              <% if session[:user_id] %>
                <li><%= link_to("Profile", user_path(current_user)) %></li>
              <% end %>
              <li><a href="/about">About</a></li>
              <li><a href="/items/BnB">BnB</a></li>
              <li><a href="/items/store">Store</a></li>
              <% if session[:user_id] %>
                <li><a href="/cart">Cart</a></li>
              <% end %>
              <% if current_user %>
                <li><%= link_to("Logout", login_path, method: :delete) %></li>
              <% else %>
                <li><%= link_to("Login", login_path) %></li>
              <% end %>
            </ul>
          </nav>
      </div>
    </header>


    <%= yield %>
  </body>
</html>
