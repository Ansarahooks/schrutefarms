<h1><%=@user.name%></h1>
<%= form_tag(orders_path, :method => :get ) do %>
    <%= submit_tag "View Order History" %>
<% end %>
<br>
<img src="<%= @user.image_url %>">

<p>Username: <%= @user.username %></p>
<br>
<p>Name: <%= @user.name %></p>
<p>Age: <%= @user.age %></p>

<%= form_tag(edit_user_path, :method => :get ) do %>
    <%= submit_tag "Edit Profile" %>
<% end %>

